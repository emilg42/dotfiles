!
! COLORSCHEME
!

! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! black
*.color0:       #282a2e
*.color8:       #373b41

! red
*.color1:       #a54242
*.color9:       #cc6666

! green
*.color2:       #8c9440
*.color10:      #b5bd68

! yellow
*.color3:       #de935f
*.color11:      #f0c674

! blue
*.color4:       #5f819d
*.color12:      #81a2be

! magenta
*.color5:       #85678f
*.color13:      #b294bb

! cyan
*.color6:       #5e8d87
*.color14:      #8abeb7

! white
*.color7:       #707880
*.color15:      #c5c8c6


!
! URXVT APPEARANCE
!

! font
URxvt.font: xft:Ubuntu\ Mono\ derivative\ Powerline:style=Regular:pixelsize=20:antialias=true:autohint=true
URxvt.boldFont: xft:Ubuntu\ Mono\ derivative\ Powerline:style=Bold:pixelsize=20:antialias=true:autohint=true

URxvt.letterSpace:       0
URxvt.lineSpace:         0devilspie -a &

! properties
URxvt.geometry:          92x24
URxvt.internalBorder:    18
URxvt.saveline:          10000

! cursor
URxvt.cursorBlink:       true
URxvt.cursorUnderline:   false

! background
URxvt.depth:             32
URxvt.fading:            20
URxvt.background:        [80]#2e3440
URxvt.fadeColor:         [80]#171a21

! additonal 
URxvt.scrollBar:         false
URxvt.urgentOnBell:      false
URxvt.iso14755:          false


!
! URXVT EXTENSIONS
!

URxvt.perl-lib:          ~/.urxvt/ext/
URxvt.perl-ext-common:   default,font-size

! font-size
URxvt.keysym.C-Up:  	font-size:increase
URxvt.keysym.C-Down: 	font-size:decrease
URxvt.keysym.C-r:   	font-size:reset

! clipboard
URxvt.keysym.Control-Meta-c:     builtin-string:
URxvt.keysym.Control-Meta-v:     builtin-string:
URxvt.keysym.Shift-Control-V:    eval:paste_clipboard
URxvt.keysym.Shift-Control-C:    eval:selection_to_clipboard
